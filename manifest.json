{
  "manifest_version": 2,

  "name": "Unified Graphics",
  "description": "This extension replaces all images on a page with unicorns.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
